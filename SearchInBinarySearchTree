class BinaryTreeNode {
     int data;
     BinaryTreeNode left;
     BinaryTreeNode right;
     BinaryTreeNode() {
         this.data = 0;
         this.left = null;
         this.right = null;
     }
     BinaryTreeNode(int data) {
         this.data = data;
         this.left = null;
         this.right = null;
     }
     BinaryTreeNode(int data, BinaryTreeNode left, BinaryTreeNode right) {
         this.data = data;
         this.left = left;
         this.right = right;
     }
 }
public class Solution {
    public static boolean searchInBST(BinaryTreeNode root, int x) {
        // Write your code here.
        while(root!=null){
            if(root.data==x){
                return true;
            }
            if(root.data>x){
                root=root.left;
            }
            else{
                root=root.right;
            }
        }
        return false;
    }
}
