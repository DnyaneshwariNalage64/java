class Solution {
    public int findUnique(int[] arr) {
        // code here
        int n=arr.length;
        for(int i=0;i<n;i++){
            int cnt=0;
            for(int j=0;j<n;j++){
                if(arr[i]==arr[j]){
                    cnt++;
                }
            }
            if (cnt==i){
            return arr[i];
        }
        
        }
        return -1;
    }
}
